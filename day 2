------------------------------------------------------------
PRINT VALUE OF BOOLEAN VARIABLE
#include <iostream>

#define True 1 

using namespace std;


int main()
{
printf("value = ",true;


}

------------------------------------------------------------
PRINT MEMORY OF DATATYPE OF BOOLEAN VARIABLE
#include <iostream>

#define True 1 

using namespace std;


int main() 
{
printf(" %d ",true;


}

-------------------------------------------------------------
PRINT  VALUES IN DESCENDING ORDER
#include <iostream>


using namespace std;


int main() 
{

int i;
 
 for(i=10;i>=1;i--)

   printf(" %d ",i);


}

--------------------------------------------------------------
PRINT ALPHABETS AND THEIR ASCII VALUES IN DESCENDING ORDER
#include <iostream>


using namespace std;


int main() 
{

int i;
 
 for(i=122;i>=97;i--)

   printf(" %c=%d ",char(i),i);


}

---------------------------------------------------------------
Write a C program to accept two integers and check whether they are equal or not. 
#include <iostream>
using namespace std;
int main()
{
   int i,j;
   scanf("%d %d",&i,&j);
   if (i==j)
    printf("number1 and number2 are equal");
    else 
    printf("number1 and number2 aren't equal");
}
write a c program to check whether the number is positive or negative.
#include <iostream>
using namespace std;
int main()
{
   int i,j;
   scanf("%d %d",&i,&j);
   if (i>0)
    printf("number is positive");
    else 
    printf("number is negative");
}
Write a C program to check whether a triangle is Equilateral, Isosceles or Scalene. 
#include <iostream>
using namespace std;
int main()
{
   int i,j,k;
   scanf("%d %d %d",&i,&j,&k);
   if (i==j==k)
    printf("this is an equilateral triangle");
    else if(i==j||j==k||k==i)
    printf("this is an isosceles trianle");
    else
    printf("this is a scalene triangle");
}
Write a C program to check whether a character is an alphabet, digit or special character. 
#include <iostream>
using namespace std;
int main()
{
   char ch;

    printf("Enter any character: ");
    scanf("%c", &ch);

    if((ch >= 'a' && ch <= 'z') || (ch >= 'A' && ch <= 'Z'))
    {
        printf("'%c' is alphabet.", ch);
    }
    else if(ch >= '0' && ch <= '9')
    {
        printf("'%c' is digit.", ch);
    }
    else 
    {
        printf("'%c' is special character.", ch);
    }
}
